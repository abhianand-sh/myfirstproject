<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Register</servlet-name>
		<servlet-class>com.ty.Register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet>
	<servlet-name>Login</servlet-name>
	<servlet-class>com.ty.LoginUser</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>Login</servlet-name>
	<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Display</servlet-name>
		<servlet-class>com.ty.DisplayUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Display</servlet-name>
		<url-pattern>/display</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Delete</servlet-name>
		<servlet-class>com.ty.DeleteUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetUser</servlet-name>
		<servlet-class>com.ty.GetUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetUser</servlet-name>
		<url-pattern>/getuser</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Update</servlet-name>
		<servlet-class>com.ty.UpdateUser</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserItem</servlet-name>
		<display-name>UserItem</display-name>
		<description></description>
		<servlet-class>com.ty.UserItem</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Update</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserItem</servlet-name>
		<url-pattern>/userItem</url-pattern>
	</servlet-mapping>
</web-app>
